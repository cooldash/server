  "author": "Imperial",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "meteor --settings settings.json",
    "client-watch": "scripts/check-client-bundle.sh && PORT=3071 node client-build/scripts/start.js",
    "client-build": "scripts/check-client-bundle.prod.sh && node client-build/scripts/build.js",
    "lint": "eslint --ext .js,.jsx .",
    "chimp-watch": "chimp --ddp=http://localhost:3000 --watch --mocha --path=tests",
    "prepare-public": "./scripts/prepare-public.sh",
    "client-bundle": "npx meteor-client bundle -c ./deploy/3081_treenity/meteor-client.config.json",
    "client-bundle-dev": "npx meteor-client bundle -d node_modules/meteor-client.dev.js",
    "analyze": "source-map-explorer build/static/js/2*",
    "prepare-meteor": "scripts/prepare-meteor.sh",
    "link-node-modules": "ln -s ./local/build/programs/server/npm/node_modules .meteor/node_modules",
    "–––––––––––––––––lingui": "echo lingui",
    "lingui:add": "lingui add-locale",
    "lingui:extract": "BABEL_ENV=extract lingui extract",
    "lingui:compile": "lingui compile",
    "–––––––––––––––––deploy": "echo deploy options",
    "deploy": "./deploy/deploy.sh 3001_erp",
    "deploy-client": "./deploy/3001_erp/deploy.sh",
    "stage": "./deploy/deploy.sh 3002_erpstage",
    "i1-mongo": "while true; do ssh -N -L 27018:localhost:27017 $I1; done",
    "–––––––––––––––––test": "echo test",
    "pretest": "npm run lint --silent",
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) { console.log('\u001b[31mPlease use yarn for installing\u001b[0m'); process.exit(1); }\"",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-watch-": "meteor test --driver-package practicalmeteor:mocha",
    "test-watch": "$NODE_DEBUG_OPTION test --driver-package meteortesting:mocha",
    "test-app-watch": "meteor test --full-app --driver-package practicalmeteor:mocha"
  },
  "meteor": {
    "mainModule": {
      "server": "imports/server/index.js"
    }
  },
